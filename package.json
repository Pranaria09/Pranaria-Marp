{
  "name": "pranaria-marp",
  "scripts": {
    "dev": "npx @marp-team/marp-cli -s ./src --theme-set ./styles/*.css",
    "build": "ls -la && rimraf public && npm run -s og-image && npm run -s deck",
    "deck": "ls -la && npx @marp-team/marp-cli --no-stdin public/src/*/index.md -o public/index.html && ncp assets public/assets",
    "og-image": "npx @marp-team/marp-cli --no-stdin ./src/*/index.md -o public/og-image.jpg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pranaria09/Pranaria-Marp.git"
  },
  "bugs": {
    "url": "https://github.com/Pranaria09/Pranaria-Marp.git/issues"
  },
  "devDependencies": {
    "@marp-team/marp-cli": "^3.4.0",
    "rimraf": "^5.0.7"
  },
  "dependencies": {
    "ncp": "^2.0.0",
    "puppeteer": "^22.12.1"
  }
}
